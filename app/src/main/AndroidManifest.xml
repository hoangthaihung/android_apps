<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jumprun.dale.com.jumprun">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="jumprun.dale.com.jumprun.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="jumprun.dale.com.jumprun.Game"
            android:label="@string/title_activity_game"
            android:screenOrientation="landscape"></activity>
        <activity
            android:name="jumprun.dale.com.jumprun.Setting"
            android:label="@string/title_activity_setting"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="jumprun.dale.com.jumprun.Highscore"
            android:label="@string/title_activity_highscore"></activity>

    </application>

</manifest>